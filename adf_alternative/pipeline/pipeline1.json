{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy geojson to CosmosDB",
				"description": "- reads geojson as is\n- could parameterise source file",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"additionalColumns": [
							{
								"name": "filepath",
								"value": "$$FILEPATH"
							},
							{
								"name": "guid",
								"value": {
									"value": "@guid()",
									"type": "Expression"
								}
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBatchTimeout": "0.00:00:30",
						"writeBatchSize": 50,
						"writeBehavior": "insert",
						"disableMetricsCollection": false,
						"maxConcurrentConnections": 1
					},
					"enableStaging": false,
					"parallelCopies": 1,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['type']"
								},
								"sink": {
									"path": "$['type']"
								}
							},
							{
								"source": {
									"path": "['geometry']"
								},
								"sink": {
									"path": "$['geometry']"
								}
							},
							{
								"source": {
									"path": "$['filepath']"
								},
								"sink": {
									"path": "$['properties']['state']"
								}
							},
							{
								"source": {
									"path": "$['guid']"
								},
								"sink": {
									"path": "$['properties']['partitionKey']"
								}
							}
						],
						"collectionReference": "$['features']"
					}
				},
				"inputs": [
					{
						"referenceName": "geojson",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CosmosDbSqlApiCollection1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"features": {
				"type": "Array"
			}
		},
		"annotations": [],
		"lastPublishTime": "2020-10-06T22:40:01Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}